
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  list-style: none;
}


html {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
body {
  /* font-family: "????" Arial, Helvetica, sans-serif; */
  font-size: 16px;
  margin: 0px auto;
  width: 100%;
  height: 100%;
  overflow: hidden;
}


h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  font-size: inherit;
}
ul,
ol {
  margin: 0;
  padding: 0;
  list-style: none;
}

p {
  margin: 0;
}

a {
  text-decoration: none;
}
